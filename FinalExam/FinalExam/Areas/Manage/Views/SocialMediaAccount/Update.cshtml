@model SocialMediaAccount
<div class="container">
    <h1 class="mb-3">Update</h1>
    <form asp-action="update" method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="form-group col-4">
                <label asp-for="Url">Url</label>
                <input asp-for="Url" class="form-control form-control-user" placeholder="Url">
                <span class="text-danger" asp-validation-for="Url"></span>
            </div>
            <div class="form-group col-4">
                <label asp-for="Icon">Icon</label>
                <input asp-for="Icon" class="form-control form-control-user" placeholder="Icon">
                <span class="text-danger" asp-validation-for="Icon"></span>
            </div>
            <div class="form-group col-4">
                <label asp-for="TeamMemberId">Team member</label>
                <select asp-for="TeamMemberId" class="form-control form-control-user"
                 asp-items="@new SelectList(ViewBag.TeamMembers,nameof(Model.TeamMemberId),nameof(Model.TeamMember.FullName))">
             </select>
                <span class="text-danger" asp-validation-for="Icon"></span>
            </div>
        </div>

        <button type="submit" class="btn btn-primary btn-user btn-block">
            Update
        </button>
    </form>
</div>